<% provide :title, t(".title") %>

<div class="content">
<h1><%= t(".title") %></h1>
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @product], url: {action: :update, id: @product.id} do |form| %>
      <%= render partial: "form", locals: {f: form} %>
    <% end %>
  </div>
</div>
