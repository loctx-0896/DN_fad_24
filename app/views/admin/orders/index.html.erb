<% provide :title, t(".title") %>
<div class="content">
  <table class="table">
    <thead class=" text-primary">
      <th>
        <%= t ".name" %>
      </th>
      <th>
        <%= t ".phone" %>
      </th>
      <th>
        <%= t ".address" %>
      </th>
      <th>
        <%= t ".order_date" %>
      </th>
      <th>
        <%= t ".date_success" %>
      </th>
      <th>
        <%= t ".status" %>
      </th>
      <th>
        <%= t ".detail" %>
      </th>
      <th>
        <%= t ".function" %>
      </th>
    </thead>
    <tbody>
      <%= render partial: "admin/orders/order", collection: @orders %>
    </tbody>
    <div class="topnav">
      <div class="search-container">
        <%= search_form_for @q, url: admin_orders_path do |f| %>
          <%= f.search_field :name_cont, placeholder: t(".search"), class: "topnav_input" %>
        <% end %>
      </div>
    </div>
  </table>
  <%= will_paginate %>
</div>
