<% provide :title, t(".title") %>
<div class="content">
  <table class="table">
    <thead class=" text-primary">
      <th>
        <%= t(".name") %>
      </th>
      <th>
        <%= t(".email") %>
      </th>
      <th>
        <%= t(".phone") %>
      </th>
      <th>
        <%= t(".food_name") %>
      </th>
      <th>
        <%= t(".picture") %>
      </th>
      <th>
        <%= t(".description") %>
      </th>
      <th>
        <%= t(".user") %>
      </th>
      <th>
        <%= t(".date_feaback") %>
      </th>
      <th>
        <%= t(".status") %>
      </th>
      <th>
        <%= t(".function") %>
      </th>
    </thead>
    <tbody>
      <%= render @contacts %>
    </tbody>
    <div class="topnav">
      <div class="search-container">
        <%= search_form_for @q, url: admin_contacts_path do |f| %>
          <%= f.search_field :name_cont, placeholder: t(".search"), class: "topnav_input" %>
        <% end %>
      </div>
    </div>
  </table>
  <%= will_paginate %>
</div>
